#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "sdmc,dr1802x", "mediatek,mt7621-soc";
	model = "SDMC DR1802X";

	aliases {
		led-boot = &led_green;
		led-failsafe = &led_red;
		led-running = &led_blue;
		led-upgrade = &led_red;
		label-mac-device = &gmac1;
	};

	chosen {
		bootargs-override = "console=ttyS0,115200";
		// bootargs-override = "console=ttyS0,115200 rootfstype=squashfs,jffs2";
	};

	leds {
		compatible = "gpio-leds";

		led_green: green {
			label = "green";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		led_blue: blue {
			label = "blue";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		led_red: red {
			label = "red";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <50>;

		reset {
			label = "reset";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};
};

&pcie {
	status = "okay";
};

&pcie1 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0>;
		mediatek,disable-radar-background;
	};
};

&gmac0 {
	nvmem-cells = <&macaddr_factory_3f806>;
	nvmem-cell-names = "mac-address";
};

&gmac1 {
	status = "okay";
	label = "wan";
	phy-handle = <&ethphy0>;

	nvmem-cells = <&macaddr_factory_3f806>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <1>;
};

&mdio {
	ethphy0: ethernet-phy@0 {
		reg = <0>;
	};
};

&switch0 {
	ports {
		port@1 {
			status = "okay";
			label = "lan";
		};
	};
};

/* &state_default {
	gpio {
		groups = "uart3";
		function = "gpio";
	};
}; */

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		// spi-max-frequency = <50000000>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Uboot";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "Uboot-env";
				reg = <0x40000 0x10000>;
				read-only;
			};

			factory: partition@50000 {
				label = "Factory";
				reg = <0x50000 0x40000>;
				read-only;
			};

			partition@90000 {
				label = "Factory-Backup";
				reg = <0x90000 0x40000>;
				read-only;
			};

			partition@d0000 {
				label = "OEM-rootfs-data";
				reg = <0xd0000 0x180000>;
				read-only;
			};

			partition@250000 {
				label = "OEM-rootfs-data_backup";
				reg = <0x250000 0x180000>;
				read-only;
			};

			partition@3d0000 {
				compatible = "denx,fit";
				label = "firmware";
				reg = <0x3d0000 0xc30000>;
			};
		};
	};
};

&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_3f806: macaddr@3f806 {
		reg = <0x3f806 0x6>;
	};
};
